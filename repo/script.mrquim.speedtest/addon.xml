<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.mrquim.speedtest"
       name="Mr. Quim Speed Tester"
       version="2.1.0"
       provider-name="Mr. Quim">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.script"
             library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Mr. Quim Internet Speed Tester</summary>
    <description lang="en">This is a fork of ARNU Box Speed Tester. No you can know how much speed your internet provider gives to you.</description>
    <platform>all</platform>
  </extension>
</addon>
